{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]
      }],
      "import/prefer-default-export": "off",
      "react-hooks/exhaustive-deps": "off",
      "no-undef": "off",
      "jsx-a11y/no-onchange": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  }